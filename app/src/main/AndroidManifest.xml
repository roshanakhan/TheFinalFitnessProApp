<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jerry.navi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="FitnessPro"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="FitnessPro"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VideoMainActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".VideoActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".feedbackMainActivity" />
        <activity android:name=".FeedbackForm"/>
        <activity android:name=".MusicPlayer" />
        <activity android:name=".Spotify" />
        <activity android:name=".bmiActivity"></activity>
        <activity android:name=".steps_counter_activity"></activity>
        <activity android:name=".caloriesActivity"></activity>
        <activity android:name=".all_timersActivity"></activity>
        <activity android:name=".tentimerActivity"></activity>
        <activity android:name=".thirtytimerActivity"></activity>
        <activity android:name=".sixtytimerActivity"></activity>

        <service android:name=".AlarmService" />

        <receiver android:name=".Alarmreceiver" />
        <receiver android:name=".NotificationClickReceiver" />

        <activity
            android:name=".RatingPart"
            android:label="@string/title_activity_rating_part"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>